{
	"compilerOptions": {
		"checkJs": true,
		"allowJs": true,
		"strict": true,
		"noImplicitAny": true,
		"target": "ES2020",
		"types": [
			"obsidian",
			"node"
		],
		"module": "commonjs",
		"lib": [
			"ES2020",
			"DOM",
			"dom.iterable"
		]
	},
	"typeAcquisition": { "enable": true, "include": ["electron"] },
	"include": [
		"**/*.js",
		"**/*.d.ts"
	]
}